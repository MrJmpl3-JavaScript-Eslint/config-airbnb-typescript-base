{
  "name": "eslint-config-airbnb-typescript-base",
  "version": "4.0.2",
  "description": "Airbnb Base Typescript config",
  "license": "MIT",
  "author": "Jos√© Manuel Casani Guerra <jmpl3.soporte@gmail.com>",
  "main": "index.js",
  "scripts": {
    "format": "npm run format:package && npm run format:prettier && npm run format:eslint",
    "format:eslint": "eslint --fix ./",
    "format:package": "prettier-package-json --write",
    "format:prettier": "prettier --write \"**/*.{js,json,md,yml}\"",
    "lint": "eslint ./",
    "validate": "npm run lint"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "confusing-browser-globals": "^1.0.11",
    "eslint": "^8.20.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0"
  },
  "devDependencies": {
    "commitlint": "^16.3.0",
    "commitlint-config-gitmoji": "^2.2.5",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.5.0",
    "prettier": "^2.7.1",
    "prettier-package-json": "^2.6.4",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint"
  }
}
